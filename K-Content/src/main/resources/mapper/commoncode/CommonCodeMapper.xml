<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.myapp.commcode.dao.ICommonCodeRepository">

<select id="findAllByUpperCommonCode" resultType="com.example.myapp.commoncode.model.CommonCode">
        SELECT * FROM TB_COMMON_CODE WHERE UPPER_COMMON_CODE = #{upperCommonCode}
    </select>
</mapper>